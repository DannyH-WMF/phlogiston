<Schema name="phabricator_tasks" metamodelVersion="4.0">
  <PhysicalSchema>
    <Table name="task_history"/>
    <Table name="phabricator_project"/>
    <Table name="phabricator_column"/>
  </PhysicalSchema>
  <Cube name="Task">
    <Dimensions>
      <Dimension name="Title" table="task_history" key="title">
      </Dimension>
      <Dimension name="Date" table="task_history" key="date">
      </Dimension>
      <Dimension name="Status" table="task_history" key="status">
      </Dimension>
      <Dimension name="Project" table="task_history" key="project">
      </Dimension>
      <Dimension name="ProjectColumn" table="task_history" key="projectcolumn">
      </Dimension>
    </Dimensions>
    <MeasureGroups>
      <MeasureGroup name="Points" table="task_history">
        <Measures>
          <Measure name="Story Points" column="points" aggregator="sum" formatString="#,###"/>
        </Measures>
      </MeasureGroup>
    </MeasureGroups>
  </Cube>
</Schema>
